      ul.guests-list .guest-item-identity { font-size: 1.2em; }
      ul.guests-list .guest-item-company { color: #888; display: inline-block; margin-right: 7px; }
      ul.guests-list .guest-item-position { color: #555; font-size: 0.8em; }
      .guests-search-form .form-control-feedback { top: 8px; right: 10px; }
